generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model humor {
  id         Int     @id @default(autoincrement())
  mood       String?
  activities String?
  date       String
}

model analysis {
  id          Int     @id @default(autoincrement())
  humorWhy    String?
  howToChange String?
}

model users {
  id       Int    @id @default(autoincrement())
  name     String @db.VarChar(50)
  email    String @unique
  password String
}
